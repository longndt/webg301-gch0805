{% extends "base.html.twig" %}

{% block body %}
   <h3>This is demo page</h3>
   {% set symfony = "Symfony framework" %}
   {% set year = 2021 %}
   <h4>{{ symfony }}</h4>
   <h4>{{ year }}</h4>
   {% set a = 5 %}
   {% set b = 10 %}
   {% set c = a + b %}
   <h5>c = {{ c }}</h5>
   {% set code = ["C", "Java", "Python", "PHP"] %}
   {% for c in code %}
      <i>{{ c }}</i> <br>
   {% endfor %}
   {% for i in range (1,20) %}
      {% if i%2==0 %}
         {{ i }}  
      {% endif %}
   {% endfor %}
   <br>
   {% for i in range (1,10,3) %}
      {{ i }}
   {% endfor %}
   <br>
   {% for i in range (15,1) %}
      {{ i }}
   {% endfor %}
   <br>
   {{ max (1,2,3,4,5) }}
   <br>
   {% set a = [30,50,20,10,40] %}
   {{ min (a) }}
   {% set city = ["HN", "HCM", "Danang"] %}
   {% for c in city %}
      {% if c|length >=3 %}
         {{ c }}
      {% endif %}
   {% endfor %}
   <br>
   {% set odd = [1,3,5,7,9] %}
   {% set even = [2,4,6,8] %}
   {% if even|length >=3 %}
      {% for e in even %}
         {{ e }}
      {% endfor %}
   {% endif %}
   {# This is a comment in TWIG #}
{% endblock %}
